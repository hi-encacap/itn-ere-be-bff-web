  "scripts": {
    "build": "nest build",
    "build:pre-push": "yarn run clean:pre-push && nest build --path ./tsconfig.pre-push.json && yarn run clean:pre-push",
    "clean:pre-push": "rimraf dist-pre-push",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:create": "yarn run typeorm migration:create",
    "migration:generate": "yarn run typeorm migration:generate -d ./src/migrations/index.ts",
    "migration:run": "yarn run typeorm migration:run -d ./src/migrations/index.ts",
    "prebuild": "rimraf dist",
    "seed": "ts-node ./src/seeders/index.ts",
    "seed:refresh": "dotenv -e .env.development -e .env.seeder ts-node ./src/seeders/index.ts",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:prod": "node dist/main",
    "start:stg": "NODE_ENV=staging yarn start",
    "test": "jest",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "version": "0.0.1",
  "packageManager": "yarn@4.9.2"
